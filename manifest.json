{
  "manifest_version": 2,

  "name": "WikiTab",
  "description": "This extension shows a Random Wikipedia Article Synopsis on new tab.",
  "version": "1.0",

  "permissions": [
    "https://en.wikipedia.org/"
  ],

  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",

  "chrome_url_overrides" : {
    "newtab": "static/views/new_wiki_tab.html"
  }
}